{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist"
  },
  "typeRoots": [
    "./types",
    "./node_modules/@types"
  ],
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules",
    "typings"
  ],
  "lib": [
    "es2015"
  ]
}